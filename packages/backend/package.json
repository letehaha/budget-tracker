  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development nodemon --exec 'ts-node -r tsconfig-paths/register' src/app.ts",
    "build": "vite build",
    "prod": "cross-env NODE_ENV=production node dist/app.js",
    "migrate:dev": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "migrate:dev:undo": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo",
    "migrate": "cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "migrate:undo": "cross-env NODE_ENV=production npx sequelize-cli db:migrate:undo",
    "migrate:generate": "cross-env NODE_ENV=development npx sequelize-cli migration:generate",
    "test": "cross-env NODE_ENV=test npm run test:unit && npm run test:e2e",
    "test:unit": "cross-env NODE_ENV=test TZ=UTC jest -c jest.config.unit.ts --passWithNoTests --forceExit --detectOpenHandles",
    "test:e2e": "cross-env NODE_ENV=test chmod +x ./src/tests/setup-e2e-tests.sh && ./src/tests/setup-e2e-tests.sh",
    "lint": "oxlint .",
    "knip": "npx knip"
  },
