    "cls-hooked": "^4.2.2",
    "config": "3.3.12",
    "connect-redis": "5.0.0",
    "cors": "2.8.5",
    "cron": "^3.2.1",
    "crypto-js": "4.2.0",
    "currency-codes": "2.1.0",
    "date-fns": "3.6.0",
    "express": "4.21.2",
    "express-validator": "7.0.1",
    "express-winston": "4.2.0",
    "jsonwebtoken": "9.0.2",
    "locale": "0.1.0",
    "morgan": "1.10.0",
    "p-queue": "6.6.2",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "pg": "8.11.5",
    "pg-hstore": "2.3.4",
    "redis": "4.7.0",
    "sequelize": "6.37.3",
    "sequelize-cli": "6.6.2",
    "sequelize-typescript": "2.1.6",
    "ts-node": "10.9.2",
    "uuid": "9.0.1",
    "winston": "3.13.0",
    "winston-loki": "^6.1.3"
